{
  "name": "travelbug",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build-android": "cd android & gradlew assembleRelease & cd ..",
    "build-android-debug": "cd android & gradlew assembleAlphaRelease & cd ..",
    "build-production-android": "cd android & gradlew assembleProdReleaseprod & cd ..",
    "build-dev-android": "cd android & gradlew assembleDev & cd ..",
    "build-with-stacktrace": "cd android & gradlew assembleRelease --stacktrace",
    "clean-build": "cd android & gradlew clean & cd ..",
    "build-android-task": "cd android & gradlew task & cd ..",
    "check-node-dependency": "npm list --depth=0",
    "react-devtools": "react-devtools",
    "react-android-assets-build": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.0.4",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-firebase/app": "^8.4.5",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "base-64": "^0.1.0",
    "firebase": "^7.23.0",
    "install": "^0.13.0",
    "querystring": "^0.2.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-elements": "^2.3.2",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-ui-lib": "^5.15.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "^0.14.0",
    "react-native-webview": "^10.9.2",
    "react-navigation": "1.6.1",
    "react-navigation-stack": "^2.8.4",
    "realm": "^10.0.1",
    "rn-contact-tracing": "0.0.3",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "jest": "^26.5.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
